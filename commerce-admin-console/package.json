{
  "name": "commerce-admin-tools",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --proxy-config proxy.conf.json --ssl --port 7443 --base-href / --deploy-url / --serve-path /",
    "build-dev": "ng build --log-level error --aot --watch",
    "build-prod": "ng build --aot --prod --deploy-url ./ --sourcemaps false --output-hashing all",
    "test-prod": "ng test --single-run --code-coverage --colors",
    "lint-prod": "ng lint --format=stylish",
    "verify": "yarn lint-prod && yarn test-prod && yarn build-prod",
    "build-api": "ng-swagger-gen",
    "create-test": "node ./scripts/createTest.js",
    "test": "ng test --code-coverage",
    "wchpush": "./node_modules/wchtools-commerce/node_modules/.bin/wchtools push",
    "wchlist": "./node_modules/wchtools-commerce/node_modules/.bin/wchtools list",
    "wchdelete": "./node_modules/wchtools-commerce/node_modules/.bin/wchtools delete"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "carbon-components": "^9.20.0",
    "core-js": "^2.5.3",
    "rxjs": "6.2.2",
    "rxjs-compat": "6.2.2",
    "zone.js": "^0.8.18",
    "carbon-components-angular": "^2.0.9"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^5.2.9",
    "@types/jasmine": "^2.8.2",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~8.0.53",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.32",
    "ng-swagger-gen": "^1.3.1",
    "protractor": "^5.4.1",
    "ts-node": "~3.3.0",
    "tslint": "^5.8.0",
    "typescript": "2.9.2"
  }
}
