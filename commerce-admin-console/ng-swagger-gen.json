{
  "$schema": "./node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "swagger": "./api_definition/swagger.json",
  "templates": "code_gen_template",
  "output": "src/app/rest",
  "config": "ng-swagger-gen.json",
  "ignoreUnusedModels": true,
  "minParamsForContainer": 2,
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": false,
  "serviceIndex": false,
  "apiModule": true,
  "enumModule": false,
  "sortParams": "none",
  "excludeTags": [
    "Contact",
    "Checkout Profile",
    "Country",
    "User Activate",
    "Store Locator",
    "Geonode"
  ]
}